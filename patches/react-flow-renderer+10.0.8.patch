+          }
+        });
+      }
+    }
+  }, [selectionKeyPressed, d3Zoom, onMove]);
+
+  useEffect(() => {
+    if (d3Zoom) {
+      if (onMoveStart) {
+        d3Zoom.on('start', (event: D3ZoomEvent<HTMLDivElement, any>) => {
+          const flowTransform = eventToFlowTransform(event.transform);
+          prevTransform.current = flowTransform;

+  onMove,
+  onMoveStart,
+  onMoveEnd,
+  zoomOnScroll = true,
+  zoomOnPinch = true,
+  panOnScroll = false,
+  panOnScrollSpeed = 0.5,
+  panOnScrollMode = PanOnScrollMode.Free,
+  zoomOnDoubleClick = true,
+  selectionKeyPressed,
+  elementsSelectable,
+  panOnDrag = true,
+  defaultPosition = [0, 0],
+  defaultZoom = 1,
+  zoomActivationKeyCode,
+  preventScrolling = true,
